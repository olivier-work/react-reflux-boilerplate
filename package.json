{
  "name": "react-reflux-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "beefy index.js:bundle.js --live --open",
    "build": "browserify index.js > bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      [
        "babelify"
      ]
    ]
  },
  "dependencies": {
    "react": "^0.13.1",
    "reflux": "^0.2.7"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "beefy": "^2.1.5",
    "browserify": "^9.0.8",
    "reactify": "^1.1.0"
  }
}
